{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "interface-name": false,
    "object-literal-sort-keys": false,
    "ordered-imports": {
      "severity": "warning"
    },
    "prefer-const": {
      "severity": "warning"
    },
    "member-ordering": {
      "severity": "warning"
    }
  }
}